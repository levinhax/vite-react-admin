{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "npm run lint:format && npm run lint:fix ",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "mock-server": "node ./mock/server.js",
    "dev:mock": "vite --mode mock",
    "test": "jest --colors",
    "test:watch": "yarn test  --coverage --collectCoverage=false --watch",
    "plop": "plop"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.18.0",
    "axios": "^0.21.1",
    "faker": "^5.5.3",
    "qiankun": "^2.6.3",
    "react": "^17.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-use": "^17.3.1",
    "video.js": "^7.17.0",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^15.14.0",
    "@types/react": "^17.0.40",
    "@types/react-custom-scrollbars": "^4.0.7",
    "@types/react-dom": "^17.0.13",
    "@types/video.js": "^7.3.29",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "json-server": "^0.16.3",
    "less": "^4.1.1",
    "lint-staged": "^11.0.0",
    "mockjs": "^1.1.0",
    "plop": "^3.0.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.2",
    "vite": "^2.3.8",
    "vite-plugin-svgr": "^0.3.0"
  }
}
